<view class="hisAttention">
  <view class="hisAttention-item" wx:for="{{list}}" wx:key="index">
    <view class="userInfo">
      <image class="avatar" src="{{item.toStudent.headimgUrl ? item.toStudent.headimgUrl :  '/imgs/icons/no_avatar.png'}}" alt=""></image>
      <text>{{item.toStudent.nickname}}</text>
    </view>
    <view class="attention-group">
      <text>{{item.bothStatus == 1 ? '互相关注'  : '已关注'}}</text>
    </view>
  </view>
</view>