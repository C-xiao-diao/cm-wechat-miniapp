Page {
  background: #f2f2f2;
}

.registerTwo-view {
  height: 100%;
  width: 750rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.registerOneSubmit {
  margin-top: 100rpx;
}

.description {
  color: #ccc;
  line-height: 40rpx;
  font-size: 24rpx;
  text-align: center;
}

.address-input {
  width: 100%;
  border: 1rpx solid #ccc;
  margin-top: 30rpx;
  text-align: left;
  border: 2rpx solid #ccc;
  height: 100rpx;
  width: 80%;
  border-radius: 10rpx;
  background-color: white;
  padding-left: 20rpx;
}

.picker {
  margin-top: 30rpx;
  text-align: left;
  padding-left: 20rpx;
}

.schoolList {
  display: flex;
  flex-direction: column;
  width: 82%;
  height: 400rpx;
  /* max-height: 400rpx; */
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
  overflow-x: hidden;
  z-index: 10;
  /* overflow: hidden; */
  position: absolute;
  top: 480rpx;
  border-radius: 10rpx;
  background-color: white;
  padding: 20rpx 40rpx;
  box-sizing: border-box;
}

.schoolList>text {
  line-height: 50rpx;
  font-size: 36rpx;
}