Page {
    background: #f9f9f9;
    font-size: 30rpx;
}

text {
    display: block;
    width: 100%;
    box-sizing: border-box;
}

.container {
    width: 100%;
    background: #eee;
    padding-bottom: 60rpx;
}

.title {
    font-size: 40rpx;
    text-align: center;
    margin: 50rpx 0;
}

.h1 {
    font-size: 34rpx;
    padding: 10rpx 20rpx 10rpx 30rpx;
}

.h2 {
    font-size: 32rpx;
    padding: 10rpx 20rpx 10rpx 60rpx;
}

.article {
    padding: 10rpx 20rpx 10rpx 80rpx;
}

.subtitle {
    padding-left: 0!important;
}

.article text {
    padding: 10rpx 20rpx 10rpx 40rpx;
}